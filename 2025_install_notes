    2  sh -c "$(curl -sSfL https://release.solana.com/v1.9.5/install)"
    3  sh -c "$(curl -sSfL https://release.anza.xyz/stable/install)"
    4  export PATH="/home/ubuntu/.local/share/solana/install/active_release/bin:$PATH"
    5  solana --version
